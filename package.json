{
  "name": "rejysten",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.{ts,tsx}",
    "fix": "pnpm run lint --fix; prettier --write src/**/*.{ts,tsx}",
    "build": "vite build",
    "dev": "vite",
    "tsc": "tsc --noEmit",
    "test": "pnpm tsc && pnpm test/nocheck",
    "test/case": "pnpm test -t",
    "test/retry": "pnpm run tsc && pnpm run test/nocheck/retry",
    "test/nocheck": "vitest run",
    "test/nocheck/retry": "for i in $(seq 3); do vitest run && exit 0; done; exit 1",
    "prepare": "git config --local core.hooksPath .githooks",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "emulator:build": "podman build -t firebase-emulator -f emulator.Containerfile",
    "emulator:run": "podman run --rm -it --network host -v $PWD/firebase.local.json:/firebase/firebase.json -v $PWD/firestore.local.rules:/firebase/firestore.rules firebase-emulator",
    "emulator:clear": "curl -X DELETE 'http://localhost:8080/emulator/v1/projects/demo/databases/(default)/documents'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youxkei/rejysten.git"
  },
  "author": "Hisayuki Mima <youxkei@gmail.com>",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/youxkei/rejysten/issues"
  },
  "homepage": "https://github.com/youxkei/rejysten#readme",
  "type": "module",
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.31.0",
    "@firebase/rules-unit-testing": "^4.0.1",
    "@solidjs/testing-library": "^0.8.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/js-yaml": "^4.0.9",
    "@types/moji": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vanilla-extract/vite-plugin": "^5.1.4",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "babel-preset-solid": "^1.9.6",
    "eslint": "^9.31.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-storybook": "^9.0.16",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.1.0",
    "playwright": "^1.54.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^9.0.17",
    "storybook-solidjs-vite": "^9.0.2",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite": "^7.3.0",
    "vite-plugin-checker": "^0.6.4",
    "vite-plugin-pwa": "^1.0.1",
    "vite-plugin-solid": "^2.11.10",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^4.0.16",
    "workbox-window": "^7.3.0"
  },
  "dependencies": {
    "@solid-primitives/context": "^0.2.3",
    "@solid-primitives/keyboard": "^1.3.3",
    "@solid-primitives/keyed": "^1.5.2",
    "@solid-primitives/scheduled": "^1.5.2",
    "@solid-primitives/storage": "^3.8.0",
    "@vanilla-extract/css": "^1.18.0",
    "fast-deep-equal": "^3.1.3",
    "firebase": "^11.10.0",
