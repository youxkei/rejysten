{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "rejysten",
  "version": "0.0.1",
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "reason-react",
    "reductive",
    "bs-css",
    "bs-css-emotion"
  ],
  "warnings": {
    "error" : "+101"
  },
  "reason": {"react-jsx": 3},
  "refmt": 3,
  "bsc-flags": ["-bs-g"]
}
